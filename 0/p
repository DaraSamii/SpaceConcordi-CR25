FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            waveTransmissive;
        gamma           1.3;
        fieldInf        100000;
        lInf            1;
        value           uniform 100000;
    }
    sides
    {
        type            freestreamPressure;
        freestreamValue uniform 100000;
        supersonic      false;
        value           uniform 100000;
    }
}


