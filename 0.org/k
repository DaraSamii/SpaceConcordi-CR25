     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/parameters.cs"

dimensions      [0 2 -2 0 0 0 0];

k   #calc "1.5 * pow((0.05 * sqrt(pow($Ux * 1.0,2) + pow($Uy * 1.0,2) + pow($Uz * 1.0,2))),2)";


internalField   uniform $k; // describes turbulence kinetic energy

boundaryField
{
    inlet
    {
         type            fixedValue;
        value           uniform $k;
    }
    outlet
    {
        type            zeroGradient;
	value           uniform $k;
    }
    sides
    {
        type            freestream;
        freestreamValue uniform $k;
    }
    rocket
    {
        type            kqRWallFunction;
        value           uniform $k;
    }
}


// ************************************************************************* //
