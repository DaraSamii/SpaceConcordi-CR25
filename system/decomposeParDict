FoamFile
{
    version         2;
    format          ascii;
    class           dictionary;
    object          decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains 27;

method          hierarchical;

hierarchicalCoeffs
{
    n               ( 3 3 3 );
    delta           0.001;
    order           xyz;
}


