FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 10;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 10;
    }
    outlet
    {
        type            zeroGradient;
    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform 10;
        value           uniform 10;
    }
    rocket
    {
        type            omegaWallFunction;
        value           uniform 1;  // Suitable value for wall function
    }
}


// ************************************************************************* //
