FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../system/parameters.cs"
dimensions      [0 1 -1 0 0 0 0];

internalField   uniform ($Ux $Uy 0);

boundaryField
{
    inlet
    {
	type            supersonicFreestream;
        pInf            $P;
        TInf            $T;
        UInf            ($Ux $Uy 0);
        gamma           1.4;
        value           uniform ($Ux $Uy 0);
}

    outlet
    {
        type            zeroGradient;
    }

    sides
    {
    type            freestream;
    freestreamValue uniform ($Ux $Uy 0);
    }

    rocket
    {
        type            noSlip;
    }
}

// ************************************************************************* //
