FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1e-5; // calculates how fast turbulent kinetic energy is dissipated into heat.

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform 1e-5;
        value           uniform 1e-5;
    }
    outlet
    {
        type            freestream;
        freestreamValue uniform 1e-5;
        value           uniform 1e-5;
    }
    sides
    {
        type            freestream;
        freestreamValue uniform 1e-5;
        value           uniform 1e-5;
    }
    rocket
    {
        type            epsilonWallFunction;
        value           uniform 1e-12;
    }
}


// ************************************************************************* //
