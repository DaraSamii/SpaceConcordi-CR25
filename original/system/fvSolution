FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "rho.*"
    {
        solver          diagonal;
    }
    
    Phi
    {
        solver          GAMG;
        smoother        symGaussSeidel;
        tolerance       1e-12;
        relTol          0.001;
        maxIter         500;
        minIter         5;
    }

    "(p|pCorr).*"
    {
        solver          PBiCGStab; // GAMG;
        smoother        symGaussSeidel;
        preconditioner  DILU; // diagonal; //DILU;
        tolerance       1e-10;
        relTol          0.001;
        maxIter         100;
        minIter         5;
    }

    "(e|h).*"
    {
        solver          PBiCGStab;
        smoother        GaussSeidel;
        nSweeps         3;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0.01;
        minIter         10;
        maxIter         10;
    }

    "(U|k|epsilon|omega|nuTilda).*"
    {
        solver      PBiCGStab; //smoothSolver;// PBiCGStab;
        smoother    GaussSeidel;
        nSweeps     2;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol      0.01;
        minIter     5;
        maxIter     20;
    }
}
SIMPLE
{
    nCorrectors      3;
    nNonOrthogonalCorrectors 2;
    
    consistent	yes;
    transonic   yes;

    pMinFactor   0.1;
    pMaxFactor   3.0;
}

PIMPLE
{
    nCorrectors      3;
    nOuterCorrectors 2;
    nNonOrthogonalCorrectors 0;
    
    consistent	no;

    pMinFactor   0.01;
    pMaxFactor   20.0;
}

potentialFlow
{
   nNonOrthogonalCorrectors  10;
}

relaxationFactors
{
   fields
   {
       p      0.5;
       U      0.5;
   }
   equations
   {
   
       U      0.4;
       "(h|e)"  0.5;
       "(k|omega|epsilon|nuTilda)"  0.5;
      //p       1;
   }
}


