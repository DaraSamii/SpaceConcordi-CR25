FoamFile
{
    format      ascii;
    class       volScalarField;
    object      T;
}
//* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../parameters.cs"

dimensions      [0 0 0 1 0 0 0];

internalField   uniform $T;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      uniform $T;
        value           uniform $T;

    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform $T;
        value           uniform $T;

    }

    sides
    {
        type            inletOutlet;
        inletValue      uniform $T;
        value           uniform $T;

    }

    rocket
    {
        zeroGradient;
    }
}

