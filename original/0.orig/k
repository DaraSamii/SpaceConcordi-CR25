FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../parameters.cs"

dimensions      [0 2 -2 0 0 0 0];

k   0.01;


internalField   uniform $k; // describes turbulence kinetic energy

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      uniform $k;
        value           uniform $k;

    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform $k;
        value           uniform $k;

    }
    sides
    {
        type            inletOutlet;
        inletValue      uniform $k;
        value           uniform $k;

    }
    rocket
    {
        type            kqRWallFunction;
        value           uniform $k;
    }
}


// ************************************************************************* //
