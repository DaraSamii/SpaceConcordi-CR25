FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

#include "../parameters.cs"


omega 20000;

internalField   uniform $omega;

boundaryField
{
    "(sides|outlet|inlet)"
    {
        //type            freestream;
        type            inletOutlet;
        inletValue      uniform $omega;
        freestreamValue      uniform $omega;
        value           uniform $omega;
    }
    rocket
    {
        type            omegaWallFunction;
        value           uniform $omega;

    }
}


